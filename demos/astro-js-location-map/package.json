{
	"name": "astro-js-location-map",
	"version": "0.0.1",
	"private": true,
	"license": "BSD-3-Clause",
	"description": "Astro JS Location Map: how you can use the Leaflet library to add a location map on your Astro site with Svelte. Handy for a contact page.",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro telemetry disable && astro build",
		"preview": "astro preview",
		"astro": "astro",
		"check": "astro check --fail-on-warnings",
		"check:svelte": "svelte-check --fail-on-hints",
		"format": "prettier --ignore-path .prettierignore --write --plugin-search-dir=. .",
		"lint:css": "stylelint \"src/**/*.{css,astro}\"",
		"prettier:check": "prettier --check --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@astrojs/svelte": "^2.0.1",
		"@types/leaflet": "^1.9.0",
		"astro": "^2.0.10",
		"leaflet": "^1.9.3",
		"postcss": "^8.4.21",
		"postcss-html": "^1.5.0",
		"prettier": "^2.8.4",
		"stylelint": "^15.0.0",
		"stylelint-config-recommended": "^10.0.1",
		"stylelint-config-standard": "^30.0.1",
		"svelte": "^3.55.1",
		"svelte-check": "^3.0.3",
		"typescript": "^4.9.5",
		"vite": "^4.1.1"
	},
	"peerDependencies": {
		"typescript": "^4.7.4",
		"vite": "^3.1.4"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/astro"
	},
