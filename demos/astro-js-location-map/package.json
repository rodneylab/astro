{
  "name": "astro-js-location-map",
  "version": "0.0.1",
  "private": true,
  "license": "BSD-3-Clause",
  "description": "Astro JS Location Map: how you can use the Leaflet library to add a location map on your Astro site with Svelte. Handy for a contact page.",
  "scripts": {
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro telemetry disable && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "check": "astro check --fail-on-warnings",
    "check:svelte": "svelte-check",
    "format": "prettier --ignore-path .prettierignore --write --plugin=prettier-plugin-astro .",
    "lint:css": "stylelint \"src/**/*.{css,astro}\"",
    "prettier:check": "prettier --check --plugin=prettier-plugin-astro ."
  },
  "devDependencies": {
    "@astrojs/check": "^0.9.4",
    "@astrojs/svelte": "^7.0.8",
    "@types/leaflet": "^1.9.17",
    "astro": "^5.5.5",
    "leaflet": "^1.9.4",
    "postcss": "^8.5.3",
    "postcss-html": "^1.8.0",
    "prettier": "^3.5.3",
    "stylelint": "^16.17.0",
    "stylelint-config-recommended": "^15.0.0",
    "stylelint-config-standard": "^37.0.0",
    "svelte": "^5.25.3",
    "svelte-check": "^4.1.5",
    "typescript": "^5.8.2",
    "vite": "^6.2.3"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/rodneylab/astro"
  },
  "bugs": {
    "url": "https://github.com/rodneylab/astro/issues"
  }
}
