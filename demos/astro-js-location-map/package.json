		"postcss-html": "^1.6.0",
		"prettier": "^3.2.5",
		"stylelint": "^16.2.1",
		"stylelint-config-recommended": "^14.0.0",
		"stylelint-config-standard": "^36.0.0",
		"svelte": "^4.2.12",
		"svelte-check": "^3.6.4",
		"typescript": "^5.3.3",
