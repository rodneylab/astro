		"prettier": "^2.8.8",
		"prettier-plugin-astro": "^0.10.0",
		"prettier-plugin-svelte": "^2.10.1",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"stylelint": "^15.7.0",
		"stylelint-config-recommended": "^12.0.0",
		"stylelint-config-standard": "^33.0.0",
		"svelte": "^3.59.1",
