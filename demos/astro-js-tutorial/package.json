{
	"name": "astro-js-tutorial",
	"version": "0.0.1",
	"private": true,
	"license": "BSD-3-Clause",
	"description": "Astro JS tutorial: a quick start guide to building your first Astro app using React and Svelte and how you can deploy your it to the cloud.",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro telemetry disable && astro build",
		"check": "astro check --fail-on-warnings",
		"preview": "astro preview",
		"format": "prettier --write --plugin=prettier-plugin-astro --plugin=prettier-plugin-svelte .",
		"lint:css": "stylelint \"src/**/*.{css,svelte}\"",
		"prettier:check": "prettier --check --plugin=prettier-plugin-astro --plugin=prettier-plugin-svelte ."
	},
	"devDependencies": {
		"@astrojs/react": "^4.2.3",
		"@astrojs/svelte": "^7.0.9",
		"@babel/core": "^7.26.10",
		"@types/react": "^19.1.0",
		"@types/react-dom": "^19.1.1",
		"astro": "5.6.1",
		"postcss": "^8.5.3",
		"postcss-html": "^1.8.0",
		"prettier": "^3.5.3",
		"prettier-plugin-astro": "^0.14.1",
		"prettier-plugin-svelte": "^3.3.3",
		"react": "^19.1.0",
		"react-dom": "^19.1.0",
		"stylelint": "^16.18.0",
		"stylelint-config-recommended": "^16.0.0",
		"stylelint-config-standard": "^38.0.0",
		"svelte": "^5.25.8",
		"typescript": "^5.8.3",
		"vite": "~6.2.5"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/astro"
	},
	"bugs": {
		"url": "https://github.com/rodneylab/astro/issues"
	}
}
