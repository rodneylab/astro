		"postcss": "^8.4.16",
		"postcss-html": "^1.5.0",
		"prettier": "^2.7.1",
		"prettier-plugin-astro": "^0.5.4",
		"prettier-plugin-svelte": "^2.7.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"stylelint": "^14.11.0",
		"stylelint-config-recommended": "^9.0.0",
		"stylelint-config-standard": "^28.0.0",
		"svelte": "^3.50.1",
