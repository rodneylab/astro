		"prettier-plugin-astro": "^0.7.2",
		"prettier-plugin-svelte": "^2.9.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"stylelint": "^14.16.1",
		"stylelint-config-recommended": "^9.0.0",
