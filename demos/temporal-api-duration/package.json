		"prettier": "^3.4.2",
		"prettier-plugin-astro": "^0.14.1",
		"stylelint": "^16.13.2",
		"stylelint-config-recommended": "^15.0.0",
		"stylelint-config-standard": "^37.0.0",
		"subfont": "^7.2.1",
		"typescript": "^5.7.3"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/astro/tree/main/demos/temporal-api-duration"
