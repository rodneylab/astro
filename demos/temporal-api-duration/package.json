		"prettier-plugin-astro": "^0.12.0",
		"stylelint": "^15.10.3",
		"stylelint-config-recommended": "^13.0.0",
		"stylelint-config-standard": "^34.0.0",
		"subfont": "^7.1.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/astro/tree/main/demos/temporal-api-duration"
	},
	"bugs": {
