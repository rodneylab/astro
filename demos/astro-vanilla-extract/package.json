{
	"name": "astro-vanilla-extract",
	"type": "module",
	"version": "0.0.1",
	"private": true,
	"license": "BSD-3-Clause",
	"description": "Astro vanilla-extract: how to set up vanilla-extract in your Astro project for zero runtime overhead styling, theming, contracts and more.",
	"scripts": {
		"dev": "astro dev",
		"start": "astro dev",
		"build": "astro telemetry disable && astro build && subfont -ir --root dist",
		"preview": "astro preview",
		"astro": "astro",
		"check": "astro check",
		"lint:css": "stylelint \"src/**/*.{css,svelte}\"",
		"format": "prettier --plugin-search-dir . --write .",
		"prettier:check": "prettier --check --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@commitlint/config-conventional": "^17.6.5",
		"@vanilla-extract/css": "^1.11.1",
		"@vanilla-extract/vite-plugin": "^3.8.2",
		"astro": "~2.6.5",
		"commitlint": "^17.6.5",
		"husky": "^8.0.3",
		"subfont": "^7.1.1",
		"typescript": "^5.1.3",
		"vite": "^4.3.9",
		"vite-imagetools": "^5.0.4"
	},
	"dependencies": {
		"@astrojs/preact": "^2.2.1",
		"@astrojs/svelte": "^3.0.0",
		"preact": "^10.15.1",
		"svelte": "^3.59.1"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/rodneylab/astro/tree/main/demos/astro-vanilla-extract"
